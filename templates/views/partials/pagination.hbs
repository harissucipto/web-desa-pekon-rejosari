<nav aria-label="Page navigation example">
	<ul class="pagination">

		<li {{#if data.posts.previous}} class="page-item" {{else}} class="page-item disabled" {{/if}}>
			<a class="page-link" href="#">Sebelumnya</a>
		</li>



		{{{paginationNavigation data.posts.pages data.posts.currentPage data.posts.totalPages}}}




		<li {{#if data.posts.next}} class="page-item" {{else}} class="page-item disabled" {{/if}}>
			<a class="page-link" href="#">Selanjutnya</a>
		</li>
	</ul>
</nav>